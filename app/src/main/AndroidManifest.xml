<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mariannelinhares.techhungry" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/friend"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/friend"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="mariannelinhares.techhungry.MainActivity" android:screenOrientation="portrait"/>
        <activity android:name="mariannelinhares.techhungry.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>